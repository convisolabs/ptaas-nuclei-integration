{
  "configurations": [
    {
      "name": "[PTaNI] Debugger",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/src/main.py",
      "console": "integratedTerminal",
      "args": [
        "-apk", "<replace_here>",
        "-pid", "<replace_here>",
        "-no", "/tmp/output.json",
        "-L", "DEBUG"
      ]
    },
    {
      "name": "[PTaNI] HML Debugger",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/src/main.py",
      "console": "integratedTerminal",
      "args": [
        "-apk", "<replace_here>",
        "-pid", "<replace_here>",
        "-no", "/tmp/output.json",
        "-L", "DEBUG",
        "-hml",
      ]
    }
  ]
}